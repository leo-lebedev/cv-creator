{
  "name": "vue-collapsed",
  "version": "1.2.6",
  "private": false,
  "description": "Dynamic CSS height transition from any to auto and vice versa for Vue 3. Accordion ready.",
  "keywords": [
    "vue",
    "vue-3",
    "vue-collapse",
    "vue-accordion",
    "collapse",
    "accordion",
    "vue-component"
  ],
  "homepage": "https://vue-collapsed.netlify.app/",
  "bugs": {
    "url": "https://github.com/smastrom/vue-collapsed/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smastrom/vue-collapsed.git"
  },
  "license": "MIT",
  "author": {
    "name": "Simone Mastromattei",
    "email": "smastrom@proton.me"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/*"
  ],
  "lint-staged": {
    "*.{ts,vue,md}": "prettier --write"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "^18.17.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "cypress": "^12.17.3",
    "cypress-wait-frames": "^0.9.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "playwright-webkit": "^1.36.2",
    "prettier": "^2.8.8",
    "rimraf": "^4.4.1",
    "typescript": "^4.9.5",
    "vite": "^4.4.8",
    "vite-plugin-dts": "^1.7.3",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  },
  "scripts": {
    "build": "rimraf dist && vite build",
    "postbuild": "pnpm pack",
    "build:app": "vite build --mode app",
    "dev": "vite",
    "test:chrome": "cypress run --component --browser chrome",
    "test:firefox": "cypress run --component --browser firefox",
    "test:webkit": "cypress run --component --browser webkit",
    "test": "pnpm test:chrome && pnpm test:firefox && pnpm test:webkit",
    "test:gui": "cypress open --component"
  }
}